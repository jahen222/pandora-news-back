{
  "kind": "collectionType",
  "collectionName": "article_categories",
  "info": {
    "name": "Article Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 64
    },
    "article_subcategories": {
      "via": "article_category",
      "collection": "article-subcategory"
    },
    "language": {
      "type": "enumeration",
      "enum": [
        "spanish",
        "english"
      ],
      "default": "spanish",
      "required": true
    }
  }
}
